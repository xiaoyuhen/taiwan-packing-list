<config>
{
  "window": {
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTextStyle": "black",
  },
  "pages": [
    "pages/home.mina"
  ]
}
</config>

<script>
import LIST_DATA from './constant'

App({
  globalData: {},

  onLaunch() {
    const checkList = wx.getStorageSync('taiwan_checkList')

    if (!checkList) {
      this.globalData.listData = LIST_DATA
    } else {
      this.globalData.listData = JSON.parse(checkList)
    }
  }
})
</script>
